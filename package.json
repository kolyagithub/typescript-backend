{
  "name": "typescript-backend",
  "version": "1.0.0",
  "description": "backend side in typescript",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-dev": "rimraf dev && rimraf -f npm-debug.log*",
    "build-client": "webpack --config src/client/config/webpack.prod.config.js  --inline --colors --progress --display-error-details",
    "build-server": "tsc --project src/server/ --outDir dist/",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "build-dev-client": "webpack --config src/client/config/webpack.dev.config.js --inline --colors --progress --display-error-details",
    "build-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap --allowJs false",
    "build-dev": "npm run clean-dev && npm run build-dev-server && npm run build-dev-client",
    "start": "node dist/server/index.js",
    "start-dev": "node dev/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "qudrat",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
