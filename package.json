    "rxjs": "5.5.2",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-router": "7.2.1",
    "koa-static": "4.0.1",
    "@koa/cors": "2.2.1",
    "koa-logger": "3.1.0",
    "zone.js": "0.8.4",
    "underscore": "1.8.3",
    "async": "2.6.0",
    "pidusage": "1.1.0",
    "socket.io": "1.4.8",
    "socket.io-client": "1.5.1",
